/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@common": [
        "src/app/common"
      ],
      "@env": [
        "src/environments/environment"
      ],
      "src/app/services/user.service": [
        "src/app/services"
      ],
      "@filters": [
        "src/app/filters"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
